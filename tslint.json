{
  "defaultSeverity": "error",
  "extends": "tslint-config-prettier",
  "rules": {
    "arrow-parens": true,
    "curly": [true, "as-needed"],
    "trailing-comma": [true, {
      "multiline": {
        "arrays": "always",
        "exports": "always",
        "imports": "always",
        "objects": "always",
        "typeLiterals": "always"
      },
      "singleline": "never",
      "esSpecCompliant": true
    }],
  }
}
